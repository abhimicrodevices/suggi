    %jmp T_0.10;
T_0.10 ;
    %pop/vec4 1;
T_0.0 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5cea68e7caa0;
T_1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cea68e8dff0_0, 0, 1;
