#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x61041c5caa00 .scope module, "srff_tb" "srff_tb" 2 1;
 .timescale 0 0;
v0x61041c5dd500_0 .var "clk", 0 0;
v0x61041c5dd5c0_0 .net "q", 0 0, v0x61041c592ba0_0;  1 drivers
v0x61041c5dd690_0 .var "r", 0 0;
v0x61041c5dd790_0 .var "s", 0 0;
S_0x61041c5cab90 .scope module, "u0" "srff" 2 8, 3 1 0, S_0x61041c5caa00;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "s";
    .port_info 2 /INPUT 1 "r";
    .port_info 3 /OUTPUT 1 "q";
v0x61041c5927b0_0 .net "clk", 0 0, v0x61041c5dd500_0;  1 drivers
v0x61041c592ba0_0 .var "q", 0 0;
v0x61041c5dd2c0_0 .net "r", 0 0, v0x61041c5dd690_0;  1 drivers
v0x61041c5dd390_0 .net "s", 0 0, v0x61041c5dd790_0;  1 drivers
E_0x61041c5911f0 .event posedge, v0x61041c5927b0_0;
    .scope S_0x61041c5cab90;
T_0 ;
    %wait E_0x61041c5911f0;
    %load/vec4 v0x61041c5dd390_0;
    %load/vec4 v0x61041c5dd2c0_0;
    %concat/vec4; draw_concat_vec4
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %jmp T_0.4;
T_0.0 ;
    %load/vec4 v0x61041c592ba0_0;
    %assign/vec4 v0x61041c592ba0_0, 0;
    %jmp T_0.4;
T_0.1 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x61041c592ba0_0, 0;
    %jmp T_0.4;
T_0.2 ;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x61041c592ba0_0, 0;
    %jmp T_0.4;
T_0.3 ;
    %load/vec4 v0x61041c592ba0_0;
    %assign/vec4 v0x61041c592ba0_0, 0;
    %jmp T_0.4;
T_0.4 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x61041c5caa00;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x61041c5dd500_0;
    %inv;
    %store/vec4 v0x61041c5dd500_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x61041c5caa00;
T_2 ;
    %vpi_call 2 20 "$dumpfile", "test_srff.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x61041c5caa00 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61041c5dd500_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61041c5dd790_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61041c5dd690_0, 0, 1;
    %delay 20, 0;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61041c5dd790_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61041c5dd690_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61041c5dd790_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61041c5dd690_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61041c5dd790_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61041c5dd690_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 41 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "srff_tb.v";
