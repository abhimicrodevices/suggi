    %cmp/u;
    %jmp/1 T_0.6, 6;
    %dup/vec4;
    %pushi/vec4 32, 0, 8;
    %cmp/u;
    %jmp/1 T_0.7, 6;
    %dup/vec4;
    %pushi/vec4 64, 0, 8;
    %cmp/u;
    %jmp/1 T_0.8, 6;
    %dup/vec4;
    %pushi/vec4 128, 0, 8;
    %cmp/u;
    %jmp/1 T_0.9, 6;
    %jmp T_0.10;
T_0.2 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x5cea68e43ba0_0, 0, 3;
    %jmp T_0.10;
T_0.3 ;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x5cea68e43ba0_0, 0, 3;
    %jmp T_0.10;
T_0.4 ;
