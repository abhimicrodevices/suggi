S_0x5ca54799a560 .scope module, "branch_tb" "branch_tb" 2 1;
 .timescale 0 0;
v0x5ca5479ac690_0 .var "branch_condition", 1 0;
v0x5ca5479ac780_0 .net "predicted_branch", 0 0, v0x5ca5479ac590_0;  1 drivers
S_0x5ca54799a6f0 .scope module, "u0" "branch" 2 7, 3 1 0, S_0x5ca54799a560;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "branch_condition";
    .port_info 1 /OUTPUT 1 "predicted_branch";
v0x5ca54795fba0_0 .net "branch_condition", 1 0, v0x5ca5479ac690_0;  1 drivers
v0x5ca5479ac4b0_0 .var "counter_state", 1 0;
v0x5ca5479ac590_0 .var "predicted_branch", 0 0;
E_0x5ca54795e720 .event edge, v0x5ca5479ac4b0_0;
E_0x5ca5479819b0 .event posedge, v0x5ca54795fba0_0;
    .scope S_0x5ca54799a6f0;
T_0 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5ca5479ac4b0_0, 0, 2;
    %end;
    .thread T_0;
    .scope S_0x5ca54799a6f0;
T_1 ;
    %wait E_0x5ca5479819b0;
    %load/vec4 v0x5ca5479ac4b0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_1.3, 6;
    %jmp T_1.4;
T_1.0 ;
    %load/vec4 v0x5ca54795fba0_0;
    %cmpi/e 0, 0, 2;
    %jmp/0xz  T_1.5, 4;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x5ca5479ac4b0_0, 0;
T_1.5 ;
    %jmp T_1.4;
T_1.1 ;
    %load/vec4 v0x5ca54795fba0_0;
    %cmpi/e 1, 0, 2;
    %jmp/0xz  T_1.7, 4;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x5ca5479ac4b0_0, 0;
    %jmp T_1.8;
T_1.7 ;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x5ca5479ac4b0_0, 0;
T_1.8 ;
    %jmp T_1.4;
T_1.2 ;
    %load/vec4 v0x5ca54795fba0_0;
    %cmpi/e 2, 0, 2;
    %jmp/0xz  T_1.9, 4;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x5ca5479ac4b0_0, 0;
    %jmp T_1.10;
T_1.9 ;
    %pushi/vec4 3, 0, 2;
    %assign/vec4 v0x5ca5479ac4b0_0, 0;
T_1.10 ;
    %jmp T_1.4;
T_1.3 ;
    %load/vec4 v0x5ca54795fba0_0;
    %cmpi/e 3, 0, 2;
    %jmp/0xz  T_1.11, 4;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x5ca5479ac4b0_0, 0;
T_1.11 ;
    %jmp T_1.4;
T_1.4 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5ca54799a6f0;
T_2 ;
    %wait E_0x5ca54795e720;
    %load/vec4 v0x5ca5479ac4b0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_2.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_2.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_2.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_2.3, 6;
    %jmp T_2.4;
T_2.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ca5479ac590_0, 0, 1;
    %jmp T_2.4;
T_2.1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ca5479ac590_0, 0, 1;
    %jmp T_2.4;
T_2.2 ;
