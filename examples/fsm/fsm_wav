#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x64f52c299b90 .scope module, "fsm_tb" "fsm_tb" 2 1;
 .timescale 0 0;
v0x64f52c2c17d0_0 .var "clk", 0 0;
v0x64f52c2c18a0_0 .net "light", 2 0, v0x64f52c298310_0;  1 drivers
v0x64f52c2c1970_0 .var "rst", 0 0;
v0x64f52c2c1a40_0 .net "state", 1 0, v0x64f52c2c1670_0;  1 drivers
S_0x64f52c2adfd0 .scope module, "uut" "fsm" 2 9, 3 1 0, S_0x64f52c299b90;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 3 "light";
    .port_info 2 /OUTPUT 2 "state";
P_0x64f52c2ae1b0 .param/l "green" 0 3 8, C4<001>;
P_0x64f52c2ae1f0 .param/l "red" 0 3 8, C4<100>;
P_0x64f52c2ae230 .param/l "s0" 0 3 7, +C4<00000000000000000000000000000000>;
P_0x64f52c2ae270 .param/l "s1" 0 3 7, +C4<00000000000000000000000000000001>;
P_0x64f52c2ae2b0 .param/l "s2" 0 3 7, +C4<00000000000000000000000000000010>;
P_0x64f52c2ae2f0 .param/l "yellow" 0 3 8, C4<010>;
v0x64f52c29a230_0 .net "clk", 0 0, v0x64f52c2c17d0_0;  1 drivers
v0x64f52c298310_0 .var "light", 2 0;
v0x64f52c2c1580_0 .var "next_state", 2 0;
v0x64f52c2c1670_0 .var "state", 1 0;
E_0x64f52c2ab7e0 .event edge, v0x64f52c2c1670_0;
E_0x64f52c2aabe0 .event posedge, v0x64f52c29a230_0;
    .scope S_0x64f52c2adfd0;
T_0 ;
    %wait E_0x64f52c2aabe0;
    %load/vec4 v0x64f52c2c1670_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x64f52c2c1580_0, 0;
    %jmp T_0.4;
T_0.0 ;
    %pushi/vec4 1, 0, 3;
    %assign/vec4 v0x64f52c2c1580_0, 0;
    %jmp T_0.4;
T_0.1 ;
    %pushi/vec4 2, 0, 3;
    %assign/vec4 v0x64f52c2c1580_0, 0;
    %jmp T_0.4;
T_0.2 ;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x64f52c2c1580_0, 0;
    %jmp T_0.4;
T_0.4 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x64f52c2adfd0;
T_1 ;
    %wait E_0x64f52c2aabe0;
    %load/vec4 v0x64f52c2c1580_0;
    %pad/u 2;
    %assign/vec4 v0x64f52c2c1670_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x64f52c2adfd0;
T_2 ;
    %wait E_0x64f52c2ab7e0;
    %load/vec4 v0x64f52c2c1670_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_2.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_2.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_2.2, 6;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x64f52c298310_0, 0, 3;
    %jmp T_2.4;
T_2.0 ;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x64f52c298310_0, 0, 3;
    %jmp T_2.4;
T_2.1 ;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x64f52c298310_0, 0, 3;
    %jmp T_2.4;
T_2.2 ;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x64f52c298310_0, 0, 3;
    %jmp T_2.4;
T_2.4 ;
    %pop/vec4 1;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x64f52c299b90;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x64f52c2c17d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x64f52c2c1970_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x64f52c299b90;
T_4 ;
    %delay 5, 0;
    %load/vec4 v0x64f52c2c17d0_0;
    %inv;
    %store/vec4 v0x64f52c2c17d0_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x64f52c299b90;
T_5 ;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x64f52c2c1970_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x64f52c2c1970_0, 0, 1;
    %end;
    .thread T_5;
    .scope S_0x64f52c299b90;
T_6 ;
    %wait E_0x64f52c2aabe0;
    %vpi_call 2 26 "$dumpfile", "test_fsm.vcd" {0 0 0};
    %vpi_call 2 27 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x64f52c299b90 {0 0 0};
    %delay 100, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %jmp T_6;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
