S_0x5c7fb7424ef0 .scope module, "hammingcode_tb" "hammingcode_tb" 2 1;
 .timescale 0 0;
v0x5c7fb744f9b0_0 .net "code_out", 6 0, v0x5c7fb73f8ba0_0;  1 drivers
v0x5c7fb744fa70_0 .var "data_in", 3 0;
v0x5c7fb744fb40_0 .net "error", 0 0, v0x5c7fb744f650_0;  1 drivers
S_0x5c7fb7425080 .scope module, "uut" "hammingcode" 2 11, 3 1 0, S_0x5c7fb7424ef0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "data_in";
    .port_info 1 /OUTPUT 7 "code_out";
    .port_info 2 /OUTPUT 1 "error";
v0x5c7fb73f8ba0_0 .var "code_out", 6 0;
v0x5c7fb744f570_0 .net "data_in", 3 0, v0x5c7fb744fa70_0;  1 drivers
v0x5c7fb744f650_0 .var "error", 0 0;
v0x5c7fb744f720_0 .var "hamming_code", 6 0;
v0x5c7fb744f800_0 .var "syndrome", 2 0;
E_0x5c7fb7424130 .event edge, v0x5c7fb73f8ba0_0, v0x5c7fb744f800_0;
E_0x5c7fb742ff30 .event edge, v0x5c7fb744f570_0;
    .scope S_0x5c7fb7425080;
T_0 ;
    %wait E_0x5c7fb742ff30;
    %load/vec4 v0x5c7fb744f570_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7fb744f720_0, 4, 1;
    %load/vec4 v0x5c7fb744f570_0;
    %parti/s 1, 2, 3;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7fb744f720_0, 4, 1;
    %load/vec4 v0x5c7fb744f570_0;
    %parti/s 1, 1, 2;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7fb744f720_0, 4, 1;
    %load/vec4 v0x5c7fb744f570_0;
    %parti/s 1, 0, 2;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7fb744f720_0, 4, 1;
    %load/vec4 v0x5c7fb744f720_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x5c7fb744f720_0;
    %parti/s 1, 2, 3;
    %xor;
    %load/vec4 v0x5c7fb744f720_0;
    %parti/s 1, 4, 4;
    %xor;
    %ix/load 4, 6, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7fb744f720_0, 4, 1;
    %load/vec4 v0x5c7fb744f720_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x5c7fb744f720_0;
    %parti/s 1, 1, 2;
    %xor;
    %load/vec4 v0x5c7fb744f720_0;
    %parti/s 1, 4, 4;
    %xor;
    %ix/load 4, 5, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7fb744f720_0, 4, 1;
    %load/vec4 v0x5c7fb744f720_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x5c7fb744f720_0;
    %parti/s 1, 1, 2;
    %xor;
    %load/vec4 v0x5c7fb744f720_0;
    %parti/s 1, 2, 3;
    %xor;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7fb744f720_0, 4, 1;
    %load/vec4 v0x5c7fb744f720_0;
    %store/vec4 v0x5c7fb73f8ba0_0, 0, 7;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5c7fb7425080;
T_1 ;
    %wait E_0x5c7fb7424130;
    %load/vec4 v0x5c7fb73f8ba0_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x5c7fb73f8ba0_0;
    %parti/s 1, 2, 3;
    %xor;
    %load/vec4 v0x5c7fb73f8ba0_0;
    %parti/s 1, 4, 4;
    %xor;
    %load/vec4 v0x5c7fb73f8ba0_0;
    %parti/s 1, 6, 4;
    %xor;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7fb744f800_0, 4, 1;
    %load/vec4 v0x5c7fb73f8ba0_0;
