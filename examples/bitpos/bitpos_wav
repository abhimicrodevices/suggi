#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5c14a3a02c00 .scope module, "bitpos_tb" "bitpos_tb" 2 1;
 .timescale 0 0;
v0x5c14a3a142c0_0 .var "binary_number", 7 0;
v0x5c14a3a14380_0 .net "bit_position", 2 0, v0x5c14a3a14180_0;  1 drivers
S_0x5c14a3a02d90 .scope module, "u0" "bitpos" 2 7, 3 1 0, S_0x5c14a3a02c00;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "binary_number";
    .port_info 1 /OUTPUT 3 "bit_position";
v0x5c14a39c9ba0_0 .net "binary_number", 7 0, v0x5c14a3a142c0_0;  1 drivers
v0x5c14a3a14180_0 .var "bit_position", 2 0;
E_0x5c14a39c88d0 .event edge, v0x5c14a39c9ba0_0;
    .scope S_0x5c14a3a02d90;
T_0 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x5c14a3a14180_0, 0, 3;
    %end;
    .thread T_0;
    .scope S_0x5c14a3a02d90;
T_1 ;
    %wait E_0x5c14a39c88d0;
    %load/vec4 v0x5c14a39c9ba0_0;
    %dup/vec4;
    %pushi/vec4 1, 0, 8;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 8;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 8;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %dup/vec4;
    %pushi/vec4 8, 0, 8;
    %cmp/u;
    %jmp/1 T_1.3, 6;
    %dup/vec4;
    %pushi/vec4 16, 0, 8;
    %cmp/u;
    %jmp/1 T_1.4, 6;
    %dup/vec4;
    %pushi/vec4 32, 0, 8;
    %cmp/u;
    %jmp/1 T_1.5, 6;
    %dup/vec4;
    %pushi/vec4 64, 0, 8;
    %cmp/u;
    %jmp/1 T_1.6, 6;
    %dup/vec4;
    %pushi/vec4 128, 0, 8;
    %cmp/u;
    %jmp/1 T_1.7, 6;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x5c14a3a14180_0, 0, 3;
    %jmp T_1.9;
T_1.0 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x5c14a3a14180_0, 0, 3;
    %jmp T_1.9;
T_1.1 ;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x5c14a3a14180_0, 0, 3;
    %jmp T_1.9;
T_1.2 ;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x5c14a3a14180_0, 0, 3;
    %jmp T_1.9;
T_1.3 ;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x5c14a3a14180_0, 0, 3;
    %jmp T_1.9;
T_1.4 ;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x5c14a3a14180_0, 0, 3;
    %jmp T_1.9;
T_1.5 ;
    %pushi/vec4 5, 0, 3;
    %store/vec4 v0x5c14a3a14180_0, 0, 3;
    %jmp T_1.9;
T_1.6 ;
    %pushi/vec4 6, 0, 3;
    %store/vec4 v0x5c14a3a14180_0, 0, 3;
    %jmp T_1.9;
T_1.7 ;
    %pushi/vec4 7, 0, 3;
    %store/vec4 v0x5c14a3a14180_0, 0, 3;
    %jmp T_1.9;
T_1.9 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5c14a3a02c00;
T_2 ;
    %vpi_call 2 15 "$dumpfile", "test_bitpos.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5c14a3a02c00 {0 0 0};
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x5c14a3a142c0_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x5c14a3a142c0_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 4, 0, 8;
    %store/vec4 v0x5c14a3a142c0_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 8, 0, 8;
    %store/vec4 v0x5c14a3a142c0_0, 0, 8;
    %delay 100, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "bitpos_tb.v";
