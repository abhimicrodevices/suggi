    %load/vec4 v0x5c7fb73f8ba0_0;
    %parti/s 1, 2, 3;
    %xor;
    %load/vec4 v0x5c7fb73f8ba0_0;
    %parti/s 1, 5, 4;
    %xor;
    %load/vec4 v0x5c7fb73f8ba0_0;
    %parti/s 1, 6, 4;
    %xor;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7fb744f800_0, 4, 1;
    %load/vec4 v0x5c7fb73f8ba0_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x5c7fb73f8ba0_0;
    %parti/s 1, 4, 4;
    %xor;
    %load/vec4 v0x5c7fb73f8ba0_0;
    %parti/s 1, 5, 4;
    %xor;
    %load/vec4 v0x5c7fb73f8ba0_0;
    %parti/s 1, 6, 4;
    %xor;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7fb744f800_0, 4, 1;
    %load/vec4 v0x5c7fb744f800_0;
    %or/r;
    %store/vec4 v0x5c7fb744f650_0, 0, 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5c7fb7424ef0;
T_2 ;
    %vpi_call 2 20 "$monitor", "Time=%0t data_in=%b code_out=%b error=%b", $time, v0x5c7fb744fa70_0, v0x5c7fb744f9b0_0, v0x5c7fb744fb40_0 {0 0 0};
