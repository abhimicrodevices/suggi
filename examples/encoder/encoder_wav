#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x61fd6d01be00 .scope module, "encoder_tb" "encoder_tb" 2 1;
 .timescale 0 0;
v0x61fd6d02dfa0_0 .var "din", 1 0;
v0x61fd6d02e090_0 .net "dout", 3 0, v0x61fd6cfe3ba0_0;  1 drivers
v0x61fd6d02e160_0 .var "en", 0 0;
S_0x61fd6d01bf90 .scope module, "u0" "encoder" 2 6, 3 1 0, S_0x61fd6d01be00;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "en";
    .port_info 1 /INPUT 2 "din";
    .port_info 2 /OUTPUT 4 "dout";
v0x61fd6cfe37b0_0 .net "din", 1 0, v0x61fd6d02dfa0_0;  1 drivers
v0x61fd6cfe3ba0_0 .var "dout", 3 0;
v0x61fd6d02de80_0 .net "en", 0 0, v0x61fd6d02e160_0;  1 drivers
E_0x61fd6cfe2670 .event edge, v0x61fd6cfe37b0_0;
    .scope S_0x61fd6d01bf90;
T_0 ;
    %wait E_0x61fd6cfe2670;
    %load/vec4 v0x61fd6cfe37b0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x61fd6cfe3ba0_0, 0, 4;
    %jmp T_0.5;
T_0.0 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x61fd6cfe3ba0_0, 0, 4;
    %jmp T_0.5;
T_0.1 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x61fd6cfe3ba0_0, 0, 4;
    %jmp T_0.5;
T_0.2 ;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x61fd6cfe3ba0_0, 0, 4;
    %jmp T_0.5;
T_0.3 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x61fd6cfe3ba0_0, 0, 4;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x61fd6d01be00;
T_1 ;
    %vpi_call 2 13 "$dumpfile", "test_encoder.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x61fd6d01be00 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x61fd6d02dfa0_0, 0, 2;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61fd6d02e160_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x61fd6d02dfa0_0, 0, 2;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61fd6d02e160_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x61fd6d02dfa0_0, 0, 2;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61fd6d02e160_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x61fd6d02dfa0_0, 0, 2;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61fd6d02e160_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "encoder_tb.v";
    "encoder.v";
